<resources>

    <style name="Widget.Theme.Maashakti.ActionBar.Fullscreen" parent="Widget.AppCompat.ActionBar">
        <item name="android:background">@color/black_overlay</item>
    </style>

    <style name="Widget.Theme.Maashakti.ButtonBar.Fullscreen" parent="">
        <item name="android:background">@color/black_overlay</item>
        <item name="android:buttonBarStyle">?android:attr/buttonBarStyle</item>
    </style>
    <style name="roundedBottomImageView4dp" parent="">
        <item name="cornerFamily">rounded</item>
        <item name="cornerSizeBottomLeft">@dimen/_16sdp</item>
        <item name="cornerSizeBottomRight">@dimen/_16sdp</item>
        <item name="cornerSizeTopRight">@dimen/_16sdp</item>
        <item name="cornerSizeTopLeft">@dimen/_16sdp</item>
    </style>
</resources>